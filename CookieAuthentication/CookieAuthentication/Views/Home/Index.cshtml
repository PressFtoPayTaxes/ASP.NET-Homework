@model CookieAuthentication.ViewModels.UserViewModel

@{
    ViewData["Title"] = "Home Page";
}

<h1>Your profile</h1>
<p><b>Full name: </b> @Model.FullName</p>
<p><b>Login: </b> @Model.Login</p>
<p><b>Age: </b> @Model.Age</p>
<p><b>Phone number: </b> @Model.PhoneNumber</p>
<a asp-action="SignOut">Sign out</a>