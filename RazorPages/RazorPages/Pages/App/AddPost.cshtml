@page
@model RazorPages.Pages.App.AddPostModel
@{
    ViewData["Title"] = "AddPost";
}

<h1>Add new post</h1>

<form method="post">
    <input type="file" asp-for="@Model.FilePath" />
    <br />
    <textarea asp-for="@Model.Description"></textarea>
    <br />
    <input type="submit" value="Add"/>
</form>

